{
  "author": "Tomeu Cabot",
  "version": "1.0.0",
  "description": "Cohere Autocomplete for Google Mail",
  "manifest_version": 3,
  "name": "Cohere",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "storage"],
  "host_permissions": ["https://mail.google.com/*"],
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "css": ["content.css"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["new_logo.png"],
      "matches": ["https://mail.google.com/*"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "new_logo.png",
      "24": "new_logo.png",
      "32": "new_logo.png"
    },
    "default_popup": "index.html"
  },
  "icons": {
    "16": "new_logo.png",
    "32": "new_logo.png",
    "48": "new_logo.png",
    "128": "new_logo.png"
  }
}
